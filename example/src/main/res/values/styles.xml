<resources>

    <!-- Used in kotlin -->
    <style name="UsedInKotlin">
        <item name="android:textColor">#000000</item>
    </style>


    <!-- Used as parent style in layout -->
    <style name="UsedInLayoutAsParentStyle1">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="UsedInLayoutAsParentStyle1Child" parent="@style/UsedInLayoutAsParentStyle1">
        <item name="android:textColor">#000000</item>
    </style>


    <!-- Used as override style in layout -->
    <style name="UsedInLayoutAsParentStyle2">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="UsedInLayoutAsParentStyle2.UsedStyle">
        <item name="android:textColor">#000000</item>
    </style>


    <!-- Unused in anywhere -->
    <style name="UnusedStyle">
        <item name="android:textColor">#000000</item>
    </style>

</resources>